{
  "expo": {
    "name": "reCar.io",
    "slug": "recario",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
   "infoPlist": {
      "NSContactsUsageDescription": "Allow access to your contacts list so you can see your friends using recario and share with them"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "Allow access to your contacts list so you can see your friends using recario and share with them"
      }
    }
  }
}